{
  "manifest_version": 3,
  "name": "Insta Story Searcher",
  "version": "1.0",
  "description": "Search for someone who viewed your story, you simp",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": ["scripting", "tabs", "activeTab", "action"],
  "host_permissions": ["https://*/*", "http://*/*"],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
